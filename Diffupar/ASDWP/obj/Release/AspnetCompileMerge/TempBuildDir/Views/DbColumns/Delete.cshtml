@model ASDWP.Models.DbColumns

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar</h2>

<h3>Se esta por eliminar la columna @Model.Name. ¿Desea continuar?</h3>

<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger" />
            @Html.ActionLink("Volver", "Index", new {idt=Model.DbtableId}, new { @class = "btn btn-info" })
        </div>
    }
</div>